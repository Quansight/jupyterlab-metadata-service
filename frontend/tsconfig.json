{
  "compilerOptions": {
    "declaration": true,
    "jsx": "react",
    "lib": ["es2015", "dom", "esnext.asynciterable"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "rootDir": "src",
    "skipLibCheck": true,
    "strictNullChecks": false,
    "target": "es2015",
    "types": [],
    "composite": true
  },
  "include": [
    "src/**/*.ts",
    "src/ui/index.tsx",
    "src/index.tsx",
    "src/ui/components/*",
    "src/metadata_explorer/index.tsx",
    "src/metadata_explorer/Header.tsx",
    "src/metadata_explorer/App.tsx",
    "src/metadata_explorer/Body.tsx"
  ],
  "references": [
    {
      "path": "../application"
    },
    {
      "path": "../apputils"
    },
    {
      "path": "../codeeditor"
    },
    {
      "path": "../completer"
    },
    {
      "path": "../coreutils"
    },
    {
      "path": "../databus"
    },
    {
      "path": "../docregistry"
    },
    {
      "path": "../launcher"
    },
    {
      "path": "../mainmenu"
    }
  ]
}
